{% extends 'base.html.twig' %}

{% block body %}

<table>
    <tr><th>Nom</th><th>Type</th><th>Action</th></tr>
{% for zone in zones %}
    <tr>
        <td>{{zone.nom}}</td>
        <td>{{zone.type.nom}}</td>
        <td><a href="{{path('app_zone_id',{'id':zone.id})}}">Modifier</a></td>
    </tr>
    
{% endfor %}
</table>
{% endblock %}
